{
  "name": "phovea_workspace",
  "description": "helper package",
  "version": "0.0.1",
  "author": {
    "name": "Caleydo Team",
    "email": "contact@caleydo.org",
    "url": "https://caleydo.org"
  },
  "license": "BSD-3-Clause",
  "private": true,
  "engines": {
    "npm": ">= 3",
    "node": ">= 6",
    "iojs": ">= 3"
  },
  "scripts": {
    "compile": "tsc",
    "lint": "tslint -c tslint.json */src/**.ts */tests/**.ts",
    "prebuild": "npm run test",
    "pretest": "npm run compile",
    "test": "karma start",
    "posttest": "npm run lint",
    "test:watch": "karma start --autoWatch=true --singleRun=false",
    "build:dev": "webpack",
    "build": "webpack --env prod",
    "watch": "webpack --watch",
    "compile:lineup_demos_source": "cd lineup_demos_source && npm run compile",
    "lint:lineup_demos_source": "cd lineup_demos_source && npm run lint",
    "lint:ts:lineup_demos_source": "cd lineup_demos_source && npm run lint:ts",
    "lint:js:lineup_demos_source": "cd lineup_demos_source && npm run lint:js",
    "docs:lineup_demos_source": "cd lineup_demos_source && npm run docs",
    "test:lineup_demos_source": "cd lineup_demos_source && npm run test",
    "test:watch:lineup_demos_source": "cd lineup_demos_source && npm run test:watch",
    "build:dev:lineup_demos_source": "cd lineup_demos_source && npm run build:dev",
    "build:lineup_demos_source": "cd lineup_demos_source && npm run build",
    "start:lineup_demos_source": "cd lineup_demos_source && npm run start",
    "start:hot:lineup_demos_source": "cd lineup_demos_source && npm run start:hot",
    "watch:lineup_demos_source": "cd lineup_demos_source && npm run watch",
    "release:major:lineup_demos_source": "cd lineup_demos_source && npm run release:major",
    "release:minor:lineup_demos_source": "cd lineup_demos_source && npm run release:minor",
    "release:patch:lineup_demos_source": "cd lineup_demos_source && npm run release:patch",
    "dist:lineup_demos_source": "cd lineup_demos_source && npm run dist",
    "docker:lineup_demos_source": "cd lineup_demos_source && npm run docker"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.41",
    "awesome-typescript-loader": "3.0.3",
    "css-loader": "0.26.1",
    "extract-loader": "0.1.0",
    "extract-text-webpack-plugin": "2.0.0-rc.3",
    "file-loader": "0.10.0",
    "html-loader": "0.4.4",
    "imports-loader": "0.7.0",
    "jasmine": "2.5.3",
    "json-loader": "0.5.4",
    "karma": "1.4.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-firefox-launcher": "1.0.0",
    "karma-jasmine": "1.1.0",
    "karma-junit-reporter": "2.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.2",
    "mkdirp": "0.5.1",
    "node-sass": "4.5.0",
    "null-loader": "0.1.1",
    "prettier": "^1.7.0",
    "raw-loader": "0.5.1",
    "sass-loader": "5.0.0",
    "style-loader": "0.13.1",
    "tslib": "1.5.0",
    "tslint": "4.4.2",
    "typedoc": "0.5.5",
    "typescript": "2.2.0",
    "url-loader": "0.5.7",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.3.0"
  },
  "dependencies": {
    "@types/d3": "3.5.36",
    "@types/file-saver": "0.0.0",
    "d3": "3.5.17",
    "file-saver": "1.3.3",
    "lineupjs": "github:Caleydo/lineupjs#master",
    "phovea_ui": "github:phovea/phovea_ui#develop",
    "phovea_core": "github:phovea/phovea_core#develop",
    "phovea_importer": "github:phovea/phovea_importer#develop"
  }
}
